{
  "name": "mukla",
  "version": "0.3.0",
  "description": "Simple and fast test runner, based on promises with basic reporter and clean stacktraces. Support custom reporters, sync, async and generator functions.",
  "repository": "tunnckoCore/mukla",
  "author": "Charlike Mike Reagent <@tunnckoCore> (http://www.tunnckocore.tk)",
  "precommit.silent": true,
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test": "standard && node test.js"
  },
  "dependencies": {
    "core-assert": "^0.2.0",
    "error-symbol": "^0.1.0",
    "extend-shallow": "^2.0.1",
    "failing-code": "github:azer/failing-code",
    "get-fn-name": "^1.0.0",
    "lazy-cache": "^2.0.1",
    "relike": "^1.0.3",
    "success-symbol": "^0.1.0"
  },
  "devDependencies": {
    "assertit": "^0.1.0",
    "capture-stream": "^0.1.2",
    "pre-commit": "*"
  },
  "files": [
    "index.js",
    "utils.js"
  ],
  "keywords": [
    "assert",
    "async",
    "asynchronous",
    "await",
    "core",
    "easy",
    "es",
    "fast",
    "fn",
    "framework",
    "function",
    "gen",
    "generator",
    "generators",
    "mukla",
    "observerable",
    "promise",
    "promises",
    "reporters",
    "runner",
    "simple",
    "small",
    "sync",
    "synchronous",
    "tdd",
    "test",
    "testing",
    "word"
  ],
  "verb": {
    "run": true,
    "toc": false,
    "layout": "empty",
    "tasks": [
      "readme"
    ],
    "lint": {
      "reflinks": true
    },
    "reflinks": [
      "core-assert",
      "assertit"
    ]
  }
}